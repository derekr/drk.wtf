  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:g": "obsidian-export garden/ app/routes/g && npm run build:manifest && npm run fmt",
    "build:manifest": "node scripts/garden-manifest.js",
    "fmt": "prettier -w .",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postbuild": "npm run fmt",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "@vercel/node": "^2.5.18",
    "highlight.js": "^11.3.1",
    "nodemon": "^2.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-highlight": "^5.0.1",
    "rehype-react": "^7.0.3",
    "rehype-rewrite": "^3.0.4",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.0",
    "remix": "^1.7.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "tiny-glob": "^0.2.9",
    "typescript": "^4.8.3"
