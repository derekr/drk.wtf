{
  "name": "drk-wtf",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && wrangler pages functions build --outdir build/worker",
    "deploy": "npm run build && wrangler deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "npx wrangler d1 migrations apply drk-db && wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "drizzle:update": "drizzle-kit generate"
  },
  "dependencies": {
    "@portabletext/react": "^3.1.0",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/client": "^6.19.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/overlays": "^2.3.52",
    "@sanity/react-loader": "^1.10.0",
    "drizzle-orm": "^0.30.10",
    "groq": "^3.44.0",
    "isbot": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@remix-run/dev": "^2.13.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.1.6",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "3.83.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
