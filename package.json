{
  "private": true,
  "name": "drk.wtf",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:g": "obsidian-export garden/ app/routes/g && npm run build:manifest && npm run fmt",
    "build:manifest": "node scripts/garden-manifest.js",
    "fmt": "prettier -w .",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postbuild": "npm run fmt",
    "cpshiki": "cp -r node_modules/shiki/themes node_modules/shiki/languages app/shiki/",
    "postinstall": "remix setup node && npm run cpshiki"
  },
  "dependencies": {
    "@portabletext/react": "^1.0.6",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/server-runtime": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/client": "^3.4.1",
    "@sanity/code-input": "3.0.0-v3-studio.9",
    "@sanity/image-url": "^1.0.1",
    "@sanity/vision": "3.0.0-dev-preview.20",
    "@vercel/node": "^2.5.21",
    "esbuild": "^0.15.10",
    "groq": "^2.33.2",
    "highlight.js": "^11.3.1",
    "radash": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-highlight": "^5.0.1",
    "rehype-react": "^7.0.3",
    "rehype-rewrite": "^3.0.4",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.0",
    "remix": "^1.7.2",
    "remix-utils": "^3.3.0",
    "sanity": "dev-preview",
    "shiki": "^0.11.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-groq": "^0.2.5",
    "tailwindcss": "^3.1.8",
    "tiny-glob": "^0.2.9",
